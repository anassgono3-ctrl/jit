{
  "name": "jit-liquidity-bot",
  "version": "1.0.0",
  "description": "JIT liquidity bot for Uniswap V3 on Ethereum",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register -r dotenv/config tests/**/*.test.ts --timeout 20000",
    "test:coverage": "nyc --reporter=text --reporter=html --reporter=lcov npm test",
    "test:hardhat": "npx hardhat test",
    "hardhat": "npx hardhat",
    "validate:contracts": "node validate-contracts.js",
    "backtest": "node -r ts-node/register src/backtest/runner.ts",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write src tests",
    "start": "node -r ts-node/register src/index.ts",
    "dev": "ts-node src/index.ts",
    "verify:numeric": "./scripts/verify-numeric.sh"
  },
  "keywords": [
    "uniswap",
    "jit",
    "liquidity",
    "defi",
    "ethereum",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "decimal.js": "^10.4.3",
    "dotenv": "^17.2.2",
    "ethers": "^6.8.0",
    "jsbi": "^4.3.0",
    "pino": "^8.15.0",
    "prom-client": "^15.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/solidity-analyzer": "^0.1.2",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "ajv": "^8.12.0",
    "chai": "^4.3.8",
    "eslint": "^8.50.0",
    "hardhat": "^2.17.0",
    "mocha": "^10.2.0",
    "nyc": "^17.1.0",
    "prettier": "^3.0.3",
    "solc": "^0.8.19",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
